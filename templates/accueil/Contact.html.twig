{% extends 'base.html.twig' %}

{% block title %} Accueil
{% endblock %}


{% block body %}

<div class="agileits-con">
    <div class="container">
        <h2 class="w3ls_head">Contact</h2>
        {% for message in app.flashes('success') %}

	<div class="alert alert-success">
		{{message}}
	</div>
{% endfor %}
        <div class="col-md-8 contact-grids1 animated wow fadeInRight" data-wow-delay=".5s">
            <form action="#" method="post">
                {{ form_start(form) }}

                    <div class="contact-form2">
                        <h4>Name</h4>
                        
                            {{form_widget(form.nom)}}
                        
                    </div>
                  
            
                    <div class="contact-form2">
                        <h4>Email</h4>
                    
                        {{form_widget(form.email)}}
                    
                    </div>
                    <div class="contact-form2">
                        <h4>Subject</h4>
                    
                        {{form_widget(form.subject)}}
                    
                    </div>
                
        
            <div class="contact-me ">
                <h4>Message</h4>
            
                {{form_widget(form.message)}}
            </div>
                    <input type="submit" value="Submit">
                    {{form_end(form)}}
            </form>
            
        </div>
        
        <div class="col-md-4 contact-grids">
            <div class=" contact-grid animated wow fadeInLeft" data-wow-delay=".5s">
                <div class="contact-grid1">
                    <div class="contact-grid2 ">
                        <i class="glyphicon glyphicon-map-marker" aria-hidden="true"></i>
                    </div>
                    
                    <div class="contact-grid3">
                        <h4>Address</h4>
                        <p>Technopôle Manouba A2 Bureau N°3 Manouba 2010 11 Rue Ziedallah Bardo Tunis 2000</p>
                    </div>
                </div>
            </div>
            <div class=" contact-grid animated wow fadeInUp" data-wow-delay=".5s">
                <div class="contact-grid1">
                    <div class="contact-grid2 contact-grid4">
                        <i class="glyphicon glyphicon-earphone" aria-hidden="true"></i>
                    </div>
                    <div class="contact-grid3">
                        <h4>Call Us</h4>
                        <p> +216 70 526 334
                        </p>
                    </div>
                </div>
            </div>
            <div class=" contact-grid animated wow fadeInRight" data-wow-delay=".5s">
                <div class="contact-grid1">
                    <div class="contact-grid2 contact-grid5">
                        <i class="glyphicon glyphicon-envelope" aria-hidden="true"></i>
                    </div>
                    <div class="contact-grid3">
                        <h4>Email</h4>
                        <p><a href="direction@sqli-services.com"> direction@sqli-services.com</a></p>
                    </div>
                </div>
            </div>
            
        </div>
    </div>
</div>


{% endblock %}